        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(heatseeker == 0)
{
    xsp = dir_x; //NORMAL
    ysp = dir_y;
}
else
{
    if(set_enemy == 0)
    {
        xsp = dir_x; //NORMAL
        ysp = dir_y;
        enemy = instance_nearest(x, y, obj_enemy_temp);
    }
    
    if(instance_exists(enemy) &amp;&amp; enemy != noone &amp;&amp; power(enemy.x-x, 2) + power(enemy.y-y, 2) &lt;= power(heat_distance, 2))
    {
        set_enemy = 1;
        if(x != enemy.x)
        {
            heat_x += sign(enemy.x - x);
            xsp += heat_x * heatseeker * movespeed;
            last_x = xsp;
        }
        else{last_x = xsp;}
        
        if(y != enemy.y)
        {
            heat_y += sign(enemy.y - y);
            ysp += heat_y * heatseeker * movespeed;
            last_y = ysp;
        }
        else{last_y = ysp;}
    }
    else if(set_enemy == 1)
    {
        dir_x = last_x;
        dir_y = last_y;
        xsp = dir_x; //NORMAL
        ysp = dir_y;
    }
}

if(grav != 0)
{
    xsp += grav_x * grav;
    ysp += grav_y * grav;
}

scr_move();
move_towards_point(x + xsp, y + ysp, 0);
image_angle = direction;
if(xsp == 0 &amp;&amp; ysp == 0){instance_destroy();}
xsp = 0;
ysp = 0;
if(place_meeting(x, y-1, obj_wall) || place_meeting(x, y+1, obj_wall) ||
   place_meeting(x-1, y, obj_wall) || place_meeting(x+1, y, obj_wall))
   {instance_destroy();}

</string>
